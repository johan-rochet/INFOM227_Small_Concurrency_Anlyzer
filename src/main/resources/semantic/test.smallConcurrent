gVar x;
gVar y;


function main () {
    if (x==2){
        thread {
                y = 4;
        }
    }
    else {
        thread {
                y = 4;
            }
    }
    thread {
        lock y;
        y = 2;
        unlock y;
    }

    thread {
        y = 4;
    }
}
function f(x1, x2) {
    var y;
    var z;
    y = x1 + x2;
    return y;
}